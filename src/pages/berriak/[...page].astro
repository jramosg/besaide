---
import News from '@/components-pages/News.astro';

import { NewsStaticPaths } from '@/staticPathsHelper';
import type { GetStaticPathsOptions } from 'astro';

export function getStaticPaths({ paginate }: GetStaticPathsOptions) {
	return NewsStaticPaths(paginate);
}

const { page } = Astro.props;
---

<News page={page} />
