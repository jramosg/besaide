---
import Pagination from './Pagination.astro';

export interface Props {
	page: AstroPage;
	class?: string;
}

import type { Page as AstroPage } from 'astro';

const { page, class: className }: Props = Astro.props;
---

<Pagination
	currentPage={page.currentPage}
	lastPage={page.lastPage}
	total={page.total}
	size={page.size}
	url={page.url}
	class={className}
/>
